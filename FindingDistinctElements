class Main{
  public static void main(string args[]){
      int arr[] = {1,1,2,3,3,4,4};
      int n = arr.lenght;
      int i=0;j=1;
      while(j<n){
        if(arr[i] != arr[j]){
            i++;
            arr[i] = arr[j];
        }
        j++;
      }
  for(int k = 0;k<=i;k++){
    System.out.print(" "+arr[k]);
  }
  }
}
